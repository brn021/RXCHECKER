{
  "id": "rosuvastatina",
  "name": "Rosuvastatina",
  "generic_name": "rosuvastatina cálcica",
  "brand_names": ["Crestor", "Rosuvastatina Generis", "Rosulip"],
  "atc_code": "C10AA07",
  "therapeutic_class": "Hipolipemiante - Inibidor HMG-CoA redutase (estatina)",
  "prescription_status": "MSRM",
  "last_updated": "2025-01-01",
  
  "drug_action": {
    "title": "Mecanismo de Ação",
    "content": "A rosuvastatina inibe competitivamente a 3-hidroxi-3-metilglutaril coenzima A (HMG-CoA) redutase, enzima limitante na biossíntese do colesterol, especialmente no fígado. Isto resulta em regulação positiva dos recetores LDL hepáticos e aumento da captação de LDL-colesterol.",
    "effects": {
      "primary": "Redução LDL-colesterol (45-63%)",
      "secondary": "Redução triglicéridos (10-35%), aumento HDL (3-14%)",
      "pleiotropic": "Efeitos anti-inflamatórios, estabilização placa aterosclerótica"
    },
    "pharmacokinetics": {
      "bioavailability": "20% (baixa)",
      "peak_effect": "3-5 horas",
      "half_life": "19 horas",
      "metabolism": "Mínimo hepático (10% CYP2C9)",
      "elimination": "90% fezes inalterada",
      "food_effect": "Redução 20% absorção (sem relevância clínica)"
    },
    "source": {
      "type": "RCM",
      "reference": "RCM Rosuvastatina - Secção 5.1"
    }
  },
  
